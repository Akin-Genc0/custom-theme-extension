{
	"name": "Elanco-Dark",
	"colors": {
		"editor.background": "#0e161e",
	"editor.foreground": "#ffffff",

	"activityBar.background": "#08171d",
	"activityBar.foreground": "#ffffff",
	"activityBar.border": "#12343b",
	"activityBarBadge.background": "#0075C9",
	"activityBarBadge.foreground": "#000000",

	"sideBar.background": "#0e161e",
	"sideBar.foreground": "#dceef4",
	"sideBar.border": "#12343b",
	"sideBarTitle.foreground": "#cfd8dc",
	"sideBarSectionHeader.background": "#10272e",
	"sideBarSectionHeader.foreground": "#e4e4e4",


	"titleBar.activeBackground": "#061319",
	"titleBar.activeForeground": "#ffffff",
	"titleBar.inactiveBackground": "#06131990",
	"titleBar.inactiveForeground": "#ffffff80",

	"tab.activeBackground": "#0a0e12",
	"tab.activeForeground": "#ffffff",
	"tab.inactiveBackground": "#101920",
	"tab.inactiveForeground": "#888888",
	"tab.border": "#00343b",
	"editorGroupHeader.tabsBackground": "#0c1e24",
	"editorGroup.border": "#00343b",

	"statusBar.background": "#003b47",
	"statusBar.foreground": "#ffffff",
	"statusBar.border": "#002c35"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#585d5f"
			}
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "#81d4fa"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#ff5252"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#597cfd"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": ["keyword.control", "punctuation", "meta.tag"],
			"settings": {
				"foreground": "#64b5f6"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag", "meta.tag.sgml"],
			"settings": {
				"foreground": "#0097a7"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": ["entity.name.function", "meta.function-call"],
			"settings": {
				"foreground": "#80cbc4"
			}
		},
		{
			"name": "Number, Constant, Argument",
			"scope": ["constant.numeric", "variable.parameter"],
			"settings": {
				"foreground": "#f78c6c"
			}
		},
		{
			"name": "String, Symbol",
			"scope": ["string", "constant.other.symbol"],
			"settings": {
				"foreground": "#aed581"
			}
		},
		{
			"name": "Class, Support",
			"scope": ["entity.name", "support.class"],
			"settings": {
				"foreground": "#ffcb6b"
			}
		},
		{
			"name": "Entity Types",
			"scope": ["support.type"],
			"settings": {
				"foreground": "#4fc3f7"
			}
		},
		{
			"name": "Sub-methods",
			"scope": ["entity.name.module.js", "variable.other.class.js"],
			"settings": {
				"foreground": "#ff8a65"
			}
		},
		{
			"name": "Language methods",
			"scope": ["variable.language"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e57373"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#c792ea"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": ["text.html.basic entity.other.attribute-name.html"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5c4dff"
			}
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#81c784"
			}
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#ef5350"
			}
		},
		{
			"name": "Changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#64b5f6"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#26c6da"
			}
		},
		{
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#4fc3f7"
			}
		},
		{
			"name": "Markdown - Heading",
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#81d4fa"
			}
		},
		{
			"name": "Markdown - Bold",
			"scope": ["markup.bold"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Markdown - Italic",
			"scope": ["markup.italic"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Markdown - Link",
			"scope": ["string.other.link.title.markdown"],
			"settings": {
				"foreground": "#64b5f6"
			}
		},
				{
		"name": "Function Variables",
		"scope": [
			"variable.other.readwrite",
			"variable.other.readwrite.ts",
			"variable.other.readwrite.js"
		],
		"settings": {
			"foreground": "#81d4fa"
		}
		},
		{
		"name": "Function Parameters",
		"scope": [
			"variable.parameter",
			"variable.parameter.function",
			"meta.function.parameters"
		],
		"settings": {
			"foreground": "#f78c6c"
		}
		},
		{
		"name": "Function Names",
		"scope": [
			"entity.name.function",
			"meta.function-call"
		],
		"settings": {
			"foreground": "#80cbc4"
		}
		},
		{
		"name": "Constants",
		"scope": [
			"variable.other.constant",
			"variable.constant"
		],
		"settings": {
			"foreground": "#c792ea"
		}
		},

		{
			"name": "Object Keys",
			"scope": [
				"meta.object-literal.key",
				"meta.object.member.key",
				"meta.object-literal.key.js",
				"meta.object-literal.key.ts",
				"meta.object-literal.key.string"
			],
			"settings": {
				"foreground": "#ffb74d"  // Choose a color that fits your theme
			}
			}

			]
			
		}

